{"id":286,"parent_id":0,"book_id":7,"content_id":1,"language_id":1,"display_order":286,"title":"Better","subtitle":"","short_title":"Better","uri":"/scriptures/tg/better","content":[["div",{"id":"t1","uri":"/scriptures/tg/better.t1","order":".000.775"},[".title",["b","Better"]],[".reference","See also",["a.scriptureRef",{"href":"/scriptures/tg/good"},"Good"],";",["a.scriptureRef",{"href":"/scriptures/tg/well-adjective-adverb"},"Well [",["i","adjective, adverb"],"]"]],["div",{"id":"f2","uri":"/scriptures/tg/better.f2","order":".000.776"},["ul.index",["li","It is",["i","better"],"that I give her to thee:",["a.scriptureRef",{"href":"/scriptures/ot/gen/29.19"},"Gen. 29:19"],";"],["li","am not I",["i","better"],"to thee than ten sons:",["a.scriptureRef",{"href":"/scriptures/ot/1-sam/1.8"},"1 Sam. 1:8"],";"],["li","thy lovingkindness is",["i","better"],"than life:",["a.scriptureRef",{"href":"/scriptures/ot/ps/63.3"},"Ps. 63:3"],";"],["li","day in thy courts is",["i","better"],"than a thousand:",["a.scriptureRef",{"href":"/scriptures/ot/ps/84.10"},["span.padWord","Ps."],"84:10"],";"],["li","It is",["i","better"],"to trust in the Lord:",["a.scriptureRef",{"href":"/scriptures/ot/ps/118.8"},["span.padWord","Ps."],"118:8"],";"],["li","wisdom is",["i","better"],"than rubies:",["a.scriptureRef",{"href":"/scriptures/ot/prov/8.11"},"Prov. 8:11"],";"],["li",["i","Better"],"is little with the fear of the Lord than:",["a.scriptureRef",{"href":"/scriptures/ot/prov/15.16"},["span.padWord","Prov."],"15:16"],";"],["li",["i","Better"],"is a little with righteousness than:",["a.scriptureRef",{"href":"/scriptures/ot/prov/16.8"},["span.padWord","Prov."],"16:8"],";"],["li","poor man is",["i","better"],"than a liar:",["a.scriptureRef",{"href":"/scriptures/ot/prov/19.22"},["span.padWord","Prov."],"19:22"],";"],["li","Two are",["i","better"],"than one:",["a.scriptureRef",{"href":"/scriptures/ot/eccl/4.9"},"Eccl. 4:9"],";"],["li","A good name is",["i","better"],"than precious ointment:",["a.scrip