{"id":984,"parent_id":0,"book_id":7,"content_id":1,"language_id":1,"display_order":984,"title":"Eternal Life","subtitle":"","short_title":"Eternal Life","uri":"/scriptures/tg/eternal-life","content":[["div",{"id":"t1","uri":"/scriptures/tg/eternal-life.t1","order":".000.3027"},[".title",["b","Eternal Life"]],[".reference","See also",["a.scriptureRef",{"href":"/scriptures/tg/earth-purpose-of"},"Earth, Purpose of"],";",["a.scriptureRef",{"href":"/scriptures/tg/election"},"Election"],";",["a.scriptureRef",{"href":"/scriptures/tg/everlasting"},"Everlasting"],";",["a.scriptureRef",{"href":"/scriptures/tg/exaltation"},"Exaltation"],";",["a.scriptureRef",{"href":"/scriptures/tg/family-eternal"},"Family, Eternal"]],["div",{"id":"f2","uri":"/scriptures/tg/eternal-life.f2","order":".000.3028"},["ul.index",["li","who among us shall dwell with",["i","everlasting"],"burnings:",["a.scriptureRef",{"href":"/scriptures/ot/isa/33.14"},"Isa. 33:14"],";"],["li","shall awake, some to",["i","everlasting"],"life:",["a.scriptureRef",{"href":"/scriptures/ot/dan/12.2"},"Dan. 12:2"],";"]]],["div",{"id":"f3","uri":"/scriptures/tg/eternal-life.f3","order":".000.3029"},["ul.index",["li","what … shall I do, that I may have",["i","eternal life"],":",["a.scriptureRef",{"href":"/scriptures/nt/matt/19.16"},"Matt. 19:16"],"; (",["a.scriptureRef",{"href":"/scriptures/nt/mark/10.17"},"Mark 10:17"],";",["a.scriptureRef",{"href":"/scriptures/nt/luke/10.25"},"Luke 10:25"],";",["a.scriptureRef",{"href":"/scriptures/nt/luke/18.18"},["span.padWord","Luke"],"18:18"],"; )"],["li","the righteous into life",["i","eternal"],":",["a.scriptureRef",{"href":"/scriptures/nt/matt/25.46"},["span.padWord","Matt."],"25:46"],";"],["li","should not perish, but have",["i","eternal life"],":",["a.scriptureRef",{"href":"/scriptures/nt/john/3.15"},"John 3:15"],";"],["li","He that believeth on the Son hath",["i","everlasting"],"life:",["a.scriptureRef",{"href":"/scriptures/nt/john/3.36"},["span.padWord","John"],"3:36"],"; (",["a.scriptureRef",{"href":"/scriptures/nt/john/6.40"},["span.padWord","John"],"6:40"],";",["a.scriptureRef",{"href":"/scriptures/nt/john/6.47"},["span.padWord","John"],"6:47"],";",["a.scriptureRef",{"href":"/scriptures/nt/matt/19.29"},"Matt. 19:29"],"; )"],["li","well of water springing up into",["i","everlasting"],"life:",["a.scriptureRef",{"href":"/scriptures/nt/john/4.14"},["span.padWord","John"],"4:14"],";"],["li","gathereth fruit unto life",["i","eternal"],":",["a.scriptureRef",{"href":"/scriptures/nt/john/4.36"},["span.padWord","John"],"4:36"],";"],["li","Search the scriptures; for in them ye think ye have",["i","eternal life"],":",["a.scriptureRef",{"href":"/scriptures/nt/john/5.39"},["span.padWord","John"],"5:39"],";"],["li","drinketh my blood, hath",["i","eternal life"],":",["a.scriptureRef",{"href":"/scriptures/nt/john/6.54"},["span.padWord","John"],"6:54"],";"],["li","thou hast the words of",["i","eternal life"],":",["a.scriptureRef",{"href":"/scriptures/nt/john/6.68"},["span.padWord","John"],"6:68"],";"],["li","I give unto them",["i","eternal life"],":",["a.scriptureRef",{"href":"/scriptures/nt/john/10.28"},["span.padWord","John"],"10:28"],";"],["li","this is life",["i","eternal"],", that they might know thee:",["a.scriptureRef",{"href":"/scriptures/nt/john/17.3"},["span.padWord","John"],"17:3"],";"],["li","ordained to",["i","eternal life"],":",["a.scriptureRef",{"href":"/scriptures/nt/acts/13.48"},"Acts 13:48"],";"],["li","seek for … immortality,",["i","eternal life"],":",["a.scriptureRef",{"href":"/scriptures/nt/rom/2.7"},"Rom. 2:7"],";"],["li","grace reign through righteousness unto",["i","eternal life"],"by Jesus:",["a.scriptureRef",{"href":"/scriptures/nt/rom/5.21"},["span.padWord","Rom."],"5:21"],";"],["li","a far more exceeding and",["i","eternal"],"weight of glory:",["a.scriptureRef",{"href":"/scriptures/nt/2-cor/4.17"},"2 Cor. 4:17"],";"],["li","we have a building of God …",["i","eternal"],"in the heavens:",["a.scriptureRef",{"href":"/scriptures/nt/2-cor/5.1"},["span.padWord","2 Cor."],"5:1"],";"],["li","lay hold on",["i","eternal life"],":",["a.scriptureRef",{"href":"/scriptures/nt/1-tim/6.12"},"1 Tim. 6:12"],";"],["li","called us unto his",["i","eternal"],"glory:",["a.scriptureRef",{"href":"/scriptures/nt/1-pet/5.10"},"1 Pet. 5:10"],";"],["li","we shall be",["i","like"],"him; for we shall see him as he is:",["a.scriptureRef",{"href":"/scriptures/nt/1-jn/3.2"},"1 Jn. 3:2"],";"],["li","no murderer hath",["i","eternal life"],":",["a.scriptureRef",{"href":"/scriptures/nt/1-jn/3.15"},["span.padWord","1 Jn."],"3:15"],";"],["li","given to us",["i","eternal life"],", and this life is in his Son:",["a.scriptureRef",{"href":"/scriptures/nt/1-jn/5.11"},["span.padWord","1 Jn."],"5:11"],";"],["li","He that overcometh shall",["i","inherit"],"all things:",["a.scriptureRef",{"href":"/scriptures/nt/rev/21.7"},"Rev. 21:7"],";"]]],["div",{"id":"f4","uri":"/scriptures/tg/eternal-life.f4","order":".000.3030"},["ul.index",["li","free to choose liberty and",["i","eternal life"],":",["a.scriptureRef",{"href":"/scriptures/bofm/2-ne/2.27"},"2 Ne. 2:27"],";"],["li","to be spiritually-minded is life",["i","eternal"],":",["a.scriptureRef",{"href":"/scriptures/bofm/2-ne/9.39"},["span.padWord","2 Ne."],"9:39"],";"],["li","narrow path which leads to",["i","eternal life"],":",["a.scriptureRef",{"href":"/scriptures/bofm/2-ne/31.18"},["span.padWord","2 Ne."],"31:18"],";"],["li","believe in him, and to endure to the end, which is life",["i","eternal"],":",["a.scriptureRef",{"href":"/scriptures/bofm/2-ne/33.4"},["span.padWord","2 Ne."],"33:4"],";"],["li","continue … until ye shall obtain",["i","eternal life"],":",["a.scriptureRef",{"href":"/scriptures/bofm/jacob/6.11"},"Jacob 6:11"],";"],["li","Thou art my servant; and … shalt have",["i","eternal life"],":",["a.scriptureRef",{"href":"/scriptures/bofm/mosiah/26.20"},"Mosiah 26:20"],";"],["li","raised to",["i","endless"],"happiness to inherit the kingdom:",["a.scriptureRef",{"href":"/scriptures/bofm/alma/41.4"},"Alma 41:4"],";"],["li","raised unto life",["i","eternal"],", and this because of your faith:",["a.scriptureRef",{"href":"/scriptures/bofm/moro/7.41"},"Moro. 7:41"],"; (",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/10.50"},"D&C 10:50"],"; )"]]],["div",{"id":"f5","uri":"/scriptures/tg/eternal-life.f5","order":".000.3031"},["ul.index",["li","he that hath",["i","eternal life"],"is rich:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/6.7"},"D&C 6:7"],";"],["li",["i","eternal life"],"… is the greatest of all the gifts of God:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/14.7"},["span.padWord","D&C"],"14:7"],"; (",["a.scriptureRef",{"href":"/scriptures/nt/rom/6.23"},"Rom. 6:23"],"; )"],["li","receive a crown of",["i","eternal life"],":",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/20.14"},["span.padWord","D&C"],"20:14"],";"],["li","power to obtain",["i","eternal life"],":",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/45.8"},["span.padWord","D&C"],"45:8"],";"],["li","they shall inherit",["i","eternal life"],":",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/50.5"},["span.padWord","D&C"],"50:5"],";"],["li","crowned with honor … immortality, and",["i","eternal life"],":",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/75.5"},["span.padWord","D&C"],"75:5"],"; (",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/138.51"},["span.padWord","D&C"],"138:51"],"; )"],["li","These shall dwell in the",["i","presence"],"of God:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/76.62"},["span.padWord","D&C"],"76:62"],";"],["li","Comforter is the promise … of",["i","eternal life"],":",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/88.4"},["span.padWord","D&C"],"88:4"],";"],["li","same sociality … coupled with",["i","eternal"],"glory:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/130.2"},["span.padWord","D&C"],"130:2"],";"],["li","This is",["i","eternal"],"lives—to know the only w