{"id":618,"parent_id":497,"book_id":1,"content_id":1,"language_id":1,"display_order":618,"title":"Psalm 121","subtitle":"","short_title":"Ps.","uri":"/scriptures/ot/ps/121","content":[["div",[".titleNumber","Psalm 121"],[".studySummary",["i","Help comes from the Lordâ€”He is the guardian of Israel."]],[".intro","A Song of degrees."]],[".bodyBlock",["div",{"id":"1","uri":"/scriptures/ot/ps/121.1","order":"02.20.121.40408"},["b","1"],"I will",["sup","a"],["a",{"href":"fn_1a"},"lift up"],"mine eyes unto the hills, from whence cometh my help."],["div",{"id":"2","uri":"/scriptures/ot/ps/121.2","order":"02.20.121.40409"},["b","2"],"My",["sup","a"],["a",{"href":"fn_2a"},"help"],["i","cometh"],"from the",["span.smallCaps","Lord"],", which made heaven and earth."],["div",{"id":"3","uri":"/scriptures/ot/ps/121.3","order":"02.20.121.40410"},["b","3"],"He will not suffer thy",["sup","a"],["a",{"href":"fn_3a"},"foot"],"to be moved: he that",["sup","b"],["a",{"href":"fn_3b"},"keepeth"],"thee will not slumber."],["div",{"id":"4","uri":"/scriptures/ot/ps/121.4","order":"02.20.121.40411"},["b","4"],"Behold, he that keepeth Israel shall neither slumber nor sleep."],["div",{"id":"5","uri":"/scriptures/ot/ps/121.5","order":"02.20.121.40412"},["b","5"],"The",["span.smallCaps","Lord"],["i","is"],"thy",["sup","a"],["a",{"href":"fn_5a"},"keeper"],": the",["span.smallCaps","Lord"],["i","is"],"thy",["sup","b"],["a",{"href":"fn_5b"},"shade"],"upon thy right hand."],["div",{"id":"6","uri":"/scriptures/ot/ps/121.6","order":"02.20.121.40413"},["b","6"],"The",["sup","a"],["a",{"href":"fn_6a"},"sun"],"shall not smite thee by day, nor the moon by night."],["div",{"id":"7","uri":"/scriptures/ot/ps/121.7","order":"02.20.121.40414"},["b","7"],"The",["span.smallCaps","Lord"],"shall preserve thee from all evil: he shall",["sup","a"],["a",{"href":"fn_7a"},"preserve"],"thy soul."],["div",{"id":"8","uri":"/scriptures/ot/ps/121.8","order":"02.20.121.40415"},["b","8"],"The",["span.smallCaps","Lord"],"shall preserve thy going out and thy",["sup","a"],["a",{"href":"fn_8a"},"coming"],"in from this time forth, and even for evermore."]]],"refs":[["div",{"id":"1a","uri":"/scriptures/ot/ps/121.1a","order":"02.20.121.40416"},["b","1a"],["a.scriptureRef",{"href":"/scriptures/ot/ps/123.1"},"Ps. 123:1"],"."],["div",{"id":"2a","uri":"/scriptures/ot/ps/121.2a","order":"02.20.121.40418"},["b","2a"],["a.scriptureRef",{"href":"/scriptures/ot/ps/124.8"},"Ps. 124:8"],"."],["div",{"id":"3a","uri":"/scriptures/ot/ps/121.3a","order":"02.20.121.40420"},["b","3a"],["a.scriptureRef",{"href":"/scriptures/ot/ps/66.9"},"Ps. 66:9"],"."],["div",{"id":"3b","uri":"/scriptures/ot/ps/121.3b","order":"02.20.121.40422"},["b","3b"],["a.scriptureRef",{"href":"/scriptures/ot/ps/127.1"},"Ps. 127:1"],"."],["div",{"id":"5a","uri":"/scriptures/ot/ps/121.5a","order":"02.20.121.40424"},["b","5a"],["span.small","OR"],"watchman."],["div",{"id":"5b","uri":"/scriptures/ot/ps/121.5b","order":"02.20.121.40426"},["b","5b"],["a.scriptureRef",{"href":"/scriptures/ot/ps/91.1"},"Ps. 91:1"],"."],["div",{"id":"6a","uri":"/scriptures/ot/ps/121.6a","order":"02.20.121.40428"},["b","6a"],["a.scriptureRef",{"href":"/scriptures/ot/isa/49.10"},"Isa. 49:10"],";",["a.scriptureRef",{"href":"/scriptures/nt/rev/7.16"},"Rev. 7:16"],"."],["div",{"id":"7a","uri":"/scriptures/ot/ps/121.7a","order":"02.20.121.40430"},["b","7a"],["a.scriptureRef",{"href":"/scriptures/ot/ps/97.10"},"Ps. 97:10"],";",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/45.46"},"D&C 45:46"],"."],["div",{"id":"8a","uri":"/scriptures/ot/ps/121.8a","order":"02.20.121.40432"},["b","8a"],["a.scriptureRef",{"href":"/scriptures/ot/deut/28.6"},"Deut. 28:6"],"."]]}
