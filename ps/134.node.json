{"id":631,"parent_id":497,"book_id":1,"content_id":1,"language_id":1,"display_order":631,"title":"Psalm 134","subtitle":"","short_title":"Ps.","uri":"/scriptures/ot/ps/134","content":[["div",[".titleNumber","Psalm 134"],[".studySummary",["i","Bless the Lord, and He will bless you."]],[".intro","A Song of degrees."]],[".bodyBlock",["div",{"id":"1","uri":"/scriptures/ot/ps/134.1","order":"02.20.134.40693"},["b","1"],"Behold, bless ye the",["span.smallCaps","Lord"],", all",["i","ye"],"servants of the",["span.smallCaps","Lord"],", which by",["sup","a"],["a",{"href":"fn_1a"},"night"],"stand in the house of the",["span.smallCaps","Lord"],"."],["div",{"id":"2","uri":"/scriptures/ot/ps/134.2","order":"02.20.134.40694"},["b","2"],"Lift up your",["sup","a"],["a",{"href":"fn_2a"},"hands"],["i","in"],"the sanctuary, and bless the",["span.smallCaps","Lord"],"."],["div",{"id":"3","uri":"/scriptures/ot/ps/134.3","order":"02.20.134.40695"},["b","3"],"The",["span.smallCaps","Lord"],"that made heaven and earth",["sup","a"],["a",{"href":"fn_3a"},"bless"],"thee out of Zion."]]],"refs":[["div",{"id":"1a","uri":"/scriptures/ot/ps/134.1a","order":"02.20.134.40696"},["b","1a"],["a.scriptureRef",{"href":"/scriptures/ot/1-chr/9.33"},"1 Chr. 9:33"],"."],["div",{"id":"2a","uri":"/scriptures/ot/ps/134.2a","order":"02.20.134.40698"},["b","2a"],["a.scriptureRef",{"href":"/scriptures/ot/ps/63.4.2-4"},"Ps. 63:4 (2–4)"],"."],["div",{"id":"3a","uri":"/scriptures/ot/ps/134.3a","order":"02.20.134.40700"},["b","3a"],["a.scriptureRef",{"href":"/scriptures/ot/ps/128.5"},"Ps. 128:5"],"."]]}
