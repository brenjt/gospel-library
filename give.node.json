","for whosoever hath, to him shall be",["i","given"],":",["a.scriptureRef",{"href":"/scriptures/nt/luke/8.18"},["span.padWord","Luke"],"8:18"],";"],["li",["i","give"],"the Holy Spirit to them that ask him:",["a.scriptureRef",{"href":"/scriptures/nt/luke/11.13"},["span.padWord","Luke"],"11:13"],";"],["li","much is",["i","given"],"… much required:",["a.scriptureRef",{"href":"/scriptures/nt/luke/12.48"},["span.padWord","Luke"],"12:48"],"; (",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/82.3"},"D&C 82:3"],"; )"],["li","my body which is",["i","given"],"for you:",["a.scriptureRef",{"href":"/scriptures/nt/luke/22.19"},["span.padWord","Luke"],"22:19"],";"],["li","he",["i","gave"],"up the ghost:",["a.scriptureRef",{"href":"/scriptures/nt/luke/23.46"},["span.padWord","Luke"],"23:46"],";"],["li","them",["i","gave"],"he power to become the sons of God:",["a.scriptureRef",{"href":"/scriptures/nt/john/1.12"},"John 1:12"],";"],["li","he",["i","gave"],"his only begotten Son:",["a.scriptureRef",{"href":"/scriptures/nt/john/3.16"},["span.padWord","John"],"3:16"],"; (",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/20.21"},"D&C 20:21"],"; )"],["li","Father loveth the Son, and hath",["i","given"],"all things into his hand:",["a.scriptureRef",{"href":"/scriptures/nt/john/3.35"},["span.padWord","John"],"3:35"],";"],["li",["i","given"],"him authority to execute judgment:",["a.scriptureRef",{"href":"/scriptures/nt/john/5.27"},["span.padWord","John"],"5:27"],";"],["li","All that the Father",["i","giveth"],"me shall come to me:",["a.scriptureRef",{"href":"/scriptures/nt/john/6.37"},["span.padWord","John"],"6:37"],";"],["li","I will",["i","give"],"for the life of the world:",["a.scriptureRef",{"href":"/scriptures/nt/john/6.51"},["span.padWord","John"],"6:51"],";"],["li","Holy Ghost was not yet",["i","given"],":",["a.scriptureRef",{"href":"/scriptures/nt/john/7.39"},["span.padWord","John"],"7:39"],";"],["li","good shepherd",["i","giveth"],"his life for the sheep:",["a.script