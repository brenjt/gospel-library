{"id":3433,"parent_id":0,"book_id":7,"content_id":1,"language_id":1,"display_order":3433,"title":"Will","subtitle":"","short_title":"Will","uri":"/scriptures/tg/will","content":[["div",{"id":"t1","uri":"/scriptures/tg/will.t1","order":".000.12017"},[".title",["b","Will"]],[".reference","See also",["a.scriptureRef",{"href":"/scriptures/tg/agency"},"Agency"],";",["a.scriptureRef",{"href":"/scriptures/tg/desire"},"Desire"],";",["a.scriptureRef",{"href":"/scriptures/tg/purpose"},"Purpose"],";",["a.scriptureRef",{"href":"/scriptures/tg/willfully"},"Willfully"],";",["a.scriptureRef",{"href":"/scriptures/tg/willing"},"Willing"],";",["a.scriptureRef",{"href":"/scriptures/tg/willingly"},"Willingly"]],["div",{"id":"f2","uri":"/scriptures/tg/will.f2","order":".000.12018"},["ul.index",["li","ye shall offer it at your own",["i","will"],":",["a.scriptureRef",{"href":"/scriptures/ot/lev/19.5"},"Lev. 19:5"],";"],["li","I delight to do thy",["i","will"],":",["a.scriptureRef",{"href":"/scriptures/ot/ps/40.8"},"Ps. 40:8"],"; (",["a.scriptureRef",{"href":"/scriptures/nt/heb/10.7,9"},"Heb. 10:7, 9"],"; )"],["li","Teach me to do thy",["i","will"],":",["a.scriptureRef",{"href":"/scriptures/ot/ps/143.10"},["span.padWord","Ps."],"143:10"],";"]]],["div",{"id":"f3","uri":"/scriptures/tg/will.f3","order":".000.12019"},["ul.index",["li","Thy",["i","will"],"be done in earth:",["a.scriptureRef",{"href":"/scriptures/nt/matt/6.10"},"Matt. 6:10"],"; (",["a.scriptureRef",{"href":"/scriptures/nt/luke/11.2"},"Luke 11:2"],";",["a.scriptureRef",{"href":"/scriptures/bofm/3-ne/13.10"},"3 Ne. 13:10"],"; )"],["li","he that doeth the",["i","will"],"of my Father:",["a.scriptureRef",{"href":"/scriptures/nt/matt/7.21"},["span.padWord","Matt."],"7:21"],"; (",["a.scriptureRef",{"href":"/scriptures/nt/matt/12.50"},["span.padWord","Matt."],"12:50"],";",["a.scriptureRef",{"href":"/scriptures/nt/mark/3.35"},"Mark 3:35"],";",["a.scriptureRef",{"href":"/scriptures/bofm/3-ne/14.21"},"3 Ne. 14:21"],"; )"],["li","it is not the",["i","will"],"of your Father:",["a.scriptureRef",{"href":"/scriptures/nt/matt/18.14"},["span.padWord","Matt."],"18:14"],";"],["li","not lawful for me to do what I",["i","will"],":",["a.scriptureRef",{"href":"/scriptures/nt/matt/20.15"},["span.padWord","Matt."],"20:15"],";"],["li","Whether of them twain did the",["i","will"],"of his father:",["a.scriptureRef",{"href":"/scriptures/nt/matt/21.31"},["span.padWord","Matt."],"21:31"],";"],["li","not as I",["i","will"],", but as thou wilt:",["a.scriptureRef",{"href":"/scriptures/nt/matt/26.39"},["span.padWord","Matt."],"26:39"],"; (",["a.scriptureRef",{"href":"/scriptures/nt/mark/14.36"},"Mark 14:36"],";",["a.scriptureRef",{"href":"/scriptures/nt/luke/22.42"},"Luke 22:42"],"; )"],["li","except I drink it, thy",["i","will"],"be done:",["a.scriptureRef",{"href":"/scriptures/nt/matt/26.42"},["span.padWord","Matt."],"26:42"],";"],["li","on earth peace, good",["i","will"],"toward men:",["a.scriptureRef",{"href":"/scriptures/nt/luke/2.14"},"Luke 2:14"],";"],["li","servant, which knew his lord’s",["i","will"],", and prepared:",["a.scriptureRef",{"href":"/scriptures/nt/luke/12.47"},["span.padWord","Luke"],"12:47"],";"],["li","born, not … of the",["i","will"],"of the flesh, nor of the",["i","will"],"of man:",["a.scriptureRef",{"href":"/scriptures/nt/john/1.13"},"John 1:13"],";"],["li","My meat is to do the",["i","will"],"of him that sent me:",["a.scriptureRef",{"href":"/scriptures/nt/john/4.34"},["span.padWord","John"],"4:34"],";"],["li","Son quickeneth whom he",["i","will"],":",["a.scriptureRef",{"href":"/scriptures/nt/john/5.21"},["span.padWord","John"],"5:21"],";"],["li","I seek not mine own",["i","will"],", but the",["i","will"],"of the Father:",["a.scriptureRef",{"href":"/scriptures/nt/john/5.30"},["span.padWord","John"],"5:30"],";"],["li","not to do mine own",["i","will"],", but the",["i","will"],"of him that sent me:",["a.scriptureRef",{"href":"/scriptures/nt/john/6.38"},["span.padWord","John"],"6:38"],";"],["li","this is the Father’s",["i","will"],":",["a.scriptureRef",{"href":"/scriptures/nt/john/6.39"},["span.padWord","John"],"6:39"],";"],["li","If any man",["i","will"],"do his",["i","will"],", he shall know of the doctrine:",["a.scriptureRef",{"href":"/scriptures/nt/john/7.17"},["span.padWord","John"],"7:17"],";"],["li","ask what ye",["i","will"],", and it shall be done:",["a.scriptureRef",{"href":"/scriptures/nt/john/15.7"},["span.padWord","John"],"15:7"],";"],["li","I",["i","will"],"that they … be with me where I am:",["a.scriptureRef",{"href":"/scriptures/nt/john/17.24"},["span.padWord","John"],"17:24"],";"],["li","If I",["i","will"],"that he tarry till I come:",["a.scriptureRef",{"href":"/scriptures/nt/john/21.22"},["span.padWord","John"],"21:22"],";"],["li",["i","will"],"of the Lord be done:",["a.scriptureRef",{"href":"/scriptures/nt/acts/21.14"},"Acts 21:14"],";"],["li","thou shouldest know his",["i","will"],":",["a.scriptureRef",{"href":"/scriptures/nt/acts/22.14"},["span.padWord","Acts"],"22:14"],";"],["li","to",["i","will"],"is present with me:",["a.scriptureRef",{"href":"/scriptures/nt/rom/7.18"},"Rom. 7:18"],";"],["li","acceptable, and perfect,",["i","will"],"of God:",["a.scriptureRef",{"href":"/scriptures/nt/rom/12.2"},["span.padWord","Rom."],"12:2"],";"],["li","hath power over his own",["i","will"],"… doeth well:",["a.scriptureRef",{"href":"/scriptures/nt/1-cor/7.37"},"1 Cor. 7:37"],";"],["li","made known unto us the mystery of his",["i","will"],":",["a.scriptureRef",{"href":"/scriptures/nt/eph/1.9"},"Eph. 1:9"],";"],["li","servants of Christ, doing the",["i","will"],"of God:",["a.scriptureRef",{"href":"/scriptures/nt/eph/6.6"},["span.padWord","Eph."],"6:6"],";"],["li","filled with the knowledge of his",["i","will"],":",["a.scriptureRef",{"href":"/scriptures/nt/col/1.9"},"Col. 1:9"],";"],["li","stand perfect and complete in all the",["i","will"],"of God:",["a.scriptureRef",{"href":"/scriptures/nt/col/4.12"},["span.padWord","Col."],"4:12"],";"],["li",["i","will"],"of God … that ye should abstain from fornication:",["a.scriptureRef",{"href":"/scriptures/nt/1-thes/4.3"},"1 Thes. 4:3"],";"],["li","God also bearing them witness … according to his own",["i","will"],":",["a.scriptureRef",{"href":"/scriptures/nt/heb/2.4"},"Heb. 2:4"],";"],["li","after ye have done the",["i","will"],"of God, ye might receive the promise:",["a.scriptureRef",{"href":"/scriptures/nt/heb/10.36"},["span.padWord","Heb."],"10:36"],";"],["li","perfect in every good work to do his",["i","will"],":",["a.scriptureRef",{"href":"/scriptures/nt/heb/13.21"},["span.padWord","Heb."],"13:21"],";"],["li","Of his own",["i","will"],"begat he us:",["a.scriptureRef",{"href":"/scriptures/nt/james/1.18"},"James 1:18"],";"],["li","ye ought to say, If the Lord",["i","will"],":",["a.scriptureRef",{"href":"/scriptures/nt/james/4.15"},["span.padWord","James"],"4:15"],";"],["li",["i","will"],"of God, that with well doing … silence the ignorance of foolish men:",["a.scriptureRef",{"href":"/scriptures/nt/1-pet/2.15"},"1 Pet. 2:15"],";"],["li","live … to the",["i","will"],"of God:",["a.scriptureRef",{"href":"/scriptures/nt/1-pet/4.2"},["span.padWord","1 Pet."],"4:2"],";"],["li","them that suffer according to the",["i","will"],"of God:",["a.scriptureRef",{"href":"/scriptures/nt/1-pet/4.19"},["span.padWord","1 Pet."],"4:19"],";"],["li","prophecy came not … by the",["i","will"],"of man:",["a.scriptureRef",{"href":"/scriptures/nt/2-pet/1.21"},"2 Pet. 1:21"],";"],["li","he that doeth the",["i","will"],"of God abideth for ever:",["a.scriptureRef",{"href":"/scriptures/nt/1-jn/2.17"},"1 Jn. 2:17"],";"],["li","if we ask any thing according to his",["i","will"],", he heareth:",["a.scriptureRef",{"href":"/scriptures/nt/1-jn/5.14"},["span.padWord","1 Jn."],"5:14"],";"],["li","God hath put in their hearts to fulfil his",["i","will"],":",["a.scriptureRef",{"href":"/scriptures/nt/rev/17.17"},"Rev. 17:17"],";"]]],["div",{"id":"f4","uri":"/scriptures/tg/will.f4","order":".000.12020"},["ul.index",["li","days … prolonged, according to the",["i","will"],"of God:",["a.scriptureRef",{"href":"/scriptures/bofm/2-ne/2.21"},"2 Ne. 2:21"],";"],["li","reconcile yourselves to the",["i","will"],"of God:",["a.scriptureRef",{"href":"/scriptures/bofm/2-ne/10.24"},["span.padWord","2 Ne."],"10:24"],";"],["li","he worketh in me to do according to his",["i","will"],":",["a.scriptureRef",{"href":"/scriptures/bofm/w-of-m/1.7"},"W of M 1:7"],";"],["li","that ye may live and move and do according to your own",["i","will"],":",["a.scriptureRef",{"href":"/scriptures/bofm/mosiah/2.21"},"Mosiah 2:21"],";"],["li","died not knowing the",["i","will"],"of God concerning them:",["a.scriptureRef",{"href":"/scriptures/bofm/mosiah/3.11"},["span.padWord","Mosiah"],"3:11"],";"],["li","subjected the flesh to the",["i","will"],"of the Father:",["a.scriptureRef",{"href":"/scriptures/bofm/mosiah/15.2"},["span.padWord","Mosiah"],"15:2"],";"],["li","Having gone according to their own carnal",["i","wills"],":",["a.scriptureRef",{"href":"/scriptures/bofm/mosiah/16.12"},["span.padWord","Mosiah"],"16:12"],";"],["li","impart of their substance of their own free",["i","will"],":",["a.scriptureRef",{"href":"/scriptures/bofm/mosiah/18.28"},["span.padWord","Mosiah"],"18:28"],";"],["li","placed in a state to act according to their",["i","wills"],":",["a.scriptureRef",{"href":"/scriptures/bofm/alma/12.31"},"Alma 12:31"],";"],["li","he allotteth unto men … according to their",["i","wills"],":",["a.scriptureRef",{"href":"/scriptures/bofm/alma/29.4"},["span.padWord","Alma"],"29:4"],";"],["li","he that knoweth the",["i","will"],"of God and doeth it not:",["a.scriptureRef",{"href":"/scriptures/bofm/alma/32.19"},["span.padWord","Alma"],"32:19"],";"],["li","became subjects to follow after their own",["i","will"],":",["a.scriptureRef",{"href":"/scriptures/bofm/alma/42.7"},["span.padWord","Alma"],"42:7"],";"],["li","at the head of government, to rule and do according to their",["i","wills"],":",["a.scriptureRef",{"href":"/scriptures/bofm/hel/7.5"},"Hel. 7:5"],";"],["li","hast sought my",["i","will"],", and to keep my commandments:",["a.scriptureRef",{"href":"/scriptures/bofm/hel/10.4"},["span.padWord","Hel."],"10:4"],";"],["li","shalt not ask that which is contrary to my",["i","will"],":",["a.scriptureRef",{"href":"/scriptures/bofm/hel/10.5"},["span.padWord","Hel."],"10:5"],";"],["li","to do the",["i","will"],", both of the Father and of the Son:",["a.scriptureRef",{"href":"/scriptures/bofm/3-ne/1.14"},"3 Ne. 1:14"],";"],["li","knew the",["i","will"],"of God … did wilfully rebel:",["a.scriptureRef",{"href":"/scriptures/bofm/3-ne/6.18"},["span.padWord","3 Ne."],"6:18"],";"],["li","I have suffered the",["i","will"],"of the Father in all things:",["a.scriptureRef",{"href":"/scriptures/bofm/3-ne/11.11"},["span.padWord","3 Ne."],"11:11"],";"],["li","came into the world to do the",["i","will"],"of my Father:",["a.scriptureRef",{"href":"/scriptures/bofm/3-ne/27.13"},["span.padWord","3 Ne."],"27:13"],";"],["li","thou hast all power, and can do whatsoever thou",["i","wilt"],":",["a.scriptureRef",{"href":"/scriptures/bofm/ether/3.4"},"Ether 3:4"],";"]]],["div",{"id":"f5","uri":"/scriptures/tg/will.f5","order":".000.12021"},["ul.index",["li","I came by the",["i","will"],"of the Father:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/19.24"},"D&C 19:24"],";"],["li","your Redeemer, by the",["i","will"],"of the Father:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/31.13"},["span.padWord","D&C"],"31:13"],";"],["li","asketh in the Spirit asketh according to the",["i","will"],"of God:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/46.30"},["span.padWord","D&C"],"46:30"],";"],["li","let God rule him that judgeth, according to the counsel of his own",["i","will"],":",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/58.20"},["span.padWord","D&C"],"58:20"],";"],["li","doeth my",["i","will"],", the same shall overcome:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/63.20"},["span.padWord","D&C"],"63:20"],";"],["li","I make known unto them the secrets of my",["i","will"],":",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/76.10"},["span.padWord","D&C"],"76:10"],";"],["li",["i","willeth"],"to abide in sin … cannot be sanctified:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/88.35"},["span.padWord","D&C"],"88:35"],";"],["li","showing forth the order and",["i","will"],"of God:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/89.2"},["span.padWord","D&C"],"89:2"],";"],["li","given you by the Holy Ghost to know my",["i","will"],":",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/124.5"},["span.padWord","D&C"],"124:5"],";"]]],["div",{"id":"f6","uri":"/scriptures/tg/will.f6","order":".000.12022"},["ul.index",["li","thy",["i","will"],"be done:",["a.scriptureRef",{"href":"/scriptures/pgp/moses/4.2"},"Moses 4:2"],";"]]]]],"refs":null}
