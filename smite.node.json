:",["a.scriptureRef",{"href":"/scriptures/ot/isa/53.4"},["span.padWord","Isa."],"53:4"],"; (",["a.scriptureRef",{"href":"/scriptures/bofm/mosiah/14.4"},"Mosiah 14:4"],"; )"],["li","ye fast … to",["i","smite"],"with the fist of wickedness:",["a.scriptureRef",{"href":"/scriptures/ot/isa/58.4"},["span.padWord","Isa."],"58:4"],";"],["li","let us",["i","smite"],"him with the tongue:",["a.scriptureRef",{"href":"/scriptures/ot/jer/18.18"},"Jer. 18:18"],";"],["li","stone …",["i","smote"],"the image upon his feet:",["a.scriptureRef",{"href":"/scriptures/ot/dan/2.34"},"Dan. 2:34"],";"],["li","Ephraim is",["i","smitten"],":",["a.scriptureRef",{"href":"/scriptures/ot/hosea/9.16"},"Hosea 9:16"],";"],["li","I",["i","smote"],"you with blasting and with mildew:",["a.scriptureRef",{"href":"/scriptures/ot/hag/2.17"},"Hag. 2:17"],";"],["li",["i","smite"],"the shepherd, and the sheep shall be scattered:",["a.scriptureRef",{"href":"/scriptures/ot/zech/13.7"},"Zech. 13:7"],"; (",["a.scriptureRef",{"href":"/scriptures/nt/matt/26.31"},"Matt. 26:31"],";",["a.scriptureRef",{"href":"/scriptures/nt/mark/14.27"},"Mark 14:27"],"; )"],["li","Lord will",["i","smite"],"all the people:",["a.scriptureRef",{"href":"/scriptures/ot/zech/14.12"},["span.padWord","Zech."],"14:12"],";"],["li","lest I come and",["i","smite"],"the earth with a curse:",["a.scriptureRef",{"href":"/scriptures/ot/mal/4.6"},"Mal. 4:6"],"; (",["a.scriptureRef",{"href":"/scriptures/bofm/3-ne/25.6"},"3 Ne. 25:6"],";",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/27.9"},"D&C 27:9"],";",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/110.15"},["span.padWord","D&C"],"110:15"],";",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/128.17"},["span.padWord","D&C"],"128:17"],";",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/138.48"},["span.padWord","D&C"],"138:48"],";",["a.scriptureRef",{"href":"/scriptures/pgp/js-h/1.39"},"JS—H 1:39"],"; )"]]],["div",{"id":"f3","uri":"/scriptures/tg/smite.f3","order":".000.9888