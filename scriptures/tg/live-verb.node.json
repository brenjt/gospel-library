{"id":1836,"parent_id":0,"book_id":7,"content_id":1,"language_id":1,"display_order":1836,"title":"Live [verb]","subtitle":"","short_title":"Live [verb]","uri":"/scriptures/tg/live-verb","content":[["div",{"id":"t1","uri":"/scriptures/tg/live-verb.t1","order":".000.6133"},[".title",["b","Live"],"[",["i","verb"],"]"],[".reference","See also",["a.scriptureRef",{"href":"/scriptures/tg/abide"},"Abide"],";",["a.scriptureRef",{"href":"/scriptures/tg/dwell"},"Dwell"],";",["a.scriptureRef",{"href":"/scriptures/tg/inhabit"},"Inhabit"]],["div",{"id":"f2","uri":"/scriptures/tg/live-verb.f2","order":".000.6134"},["ul.index",["li","tree of life, and eat, and",["i","live"],"for ever:",["a.scriptureRef",{"href":"/scriptures/ot/gen/3.22"},"Gen. 3:22"],"; (",["a.scriptureRef",{"href":"/scriptures/pgp/moses/4.28"},"Moses 4:28"],"; )"],["li","there shall no man see me, and",["i","live"],":",["a.scriptureRef",{"href":"/scriptures/ot/ex/33.20"},"Ex. 33:20"],";"],["li","keep my statutes … which if a man do, he shall",["i","live"],"in them:",["a.scriptureRef",{"href":"/scriptures/ot/lev/18.5"},"Lev. 18:5"],"; (",["a.scriptureRef",{"href":"/scriptures/nt/rom/10.5"},"Rom. 10:5"],";",["a.scriptureRef",{"href":"/scriptures/nt/gal/3.12"},"Gal. 3:12"],"; )"],["li","when he beheld the serpent of brass, he",["i","lived"],":",["a.scriptureRef",{"href":"/scriptures/ot/num/21.9"},"Num. 21:9"],";"],["li","God doth talk with man, and he",["i","liveth"],":",["a.scriptureRef",{"href":"/scriptures/ot/deut/5.24"},"Deut. 5:24"],";"],["li","man doth not",["i","live"],"by bread:",["a.scriptureRef",{"href":"/scriptures/ot/deut/8.3"},["span.padWord","Deut."],"8:3"],"; (",["a.scriptureRef",{"href":"/scriptures/nt/matt/4.4"},"Matt. 4:4"],";",["a.scriptureRef",{"href":"/scriptures/nt/luke/4.4"},"Luke 4:4"],"; )"],["li","If a man die, shall he",["i","live"],"again:",["a.scriptureRef",{"href":"/scriptures/ot/job/14.14"},"Job 14:14"],";"],["li","I know that my redeemer",["i","liveth"],":",["a.scriptureRef",{"href":"/