{"id":2440,"parent_id":0,"book_id":7,"content_id":1,"language_id":1,"display_order":2440,"title":"Prove","subtitle":"","short_title":"Prove","uri":"/scriptures/tg/prove","content":[["div",{"id":"t1","uri":"/scriptures/tg/prove.t1","order":".000.8279"},[".title",["b","Prove"]],[".reference","See also",["a.scriptureRef",{"href":"/scriptures/tg/examine"},"Examine"],";",["a.scriptureRef",{"href":"/scriptures/tg/temptation"},"Temptation"],";",["a.scriptureRef",{"href":"/scriptures/tg/test"},"Test"],";",["a.scriptureRef",{"href":"/scriptures/tg/try"},"Try"]],["div",{"id":"f2","uri":"/scriptures/tg/prove.f2","order":".000.8280"},["ul.index",["li","ye shall be",["i","proved"],":",["a.scriptureRef",{"href":"/scriptures/ot/gen/42.15"},"Gen. 42:15"],";"],["li",["i","prove"],"them, whether they will walk in my law:",["a.scriptureRef",{"href":"/scriptures/ot/ex/16.4"},"Ex. 16:4"],";"],["li","God is come to",["i","prove"],"you:",["a.scriptureRef",{"href":"/scriptures/ot/ex/20.20"},["span.padWord","Ex."],"20:20"],";"],["li","God led thee … in the wilderness … to",["i","prove"],"thee:",["a.scriptureRef",{"href":"/scriptures/ot/deut/8.2"},"Deut. 8:2"],";"],["li","that he might",["i","prove"],"thee, to do thee good:",["a.scriptureRef",{"href":"/scriptures/ot/deut/8.16"},["span.padWord","Deut."],"8:16"],";"],["li","God",["i","proveth"],"you, to know whether ye love the Lord:",["a.scriptureRef",{"href":"/scriptures/ot/deut/13.3"},["span.padWord","Deut."],"13:3"],";"],["li","through them I may",["i","prove"],"Israel:",["a.scriptureRef",{"href":"/scriptures/ot/judg/2.22"},"Judg. 2:22"],";"],["li","David … had not",["i","proved"],"it:",["a.scriptureRef",{"href":"/scriptures/ot/1-sam/17.39"},"1 Sam. 17:39"],";"],["li","queen of Sheba … came to",["i","prove"],"him:",["a.scriptureRef",{"href":"/scriptures/ot/1-kgs/10.1"},"1 Kgs. 10:1"],"; (",["a.scriptureRef",{"href":"/scriptures/ot/2-chr/9.1"},"2 Chr. 9:1"],"; )"],["li","Examine me, O Lord, and",["i","prove"],"me:",["a.scriptureRef",{"href":"/scriptures/ot/ps/26.2"},"Ps. 26:2"],";"],["li","I",["i","proved"],"thee at the waters of Meribah:",["a.scriptureRef",{"href":"/scriptures/ot/ps/81.7"},["span.padWord","Ps."],"81:7"],";"],["li","your fathers tempted me,",["i","proved"],"me:",["a.scriptureRef",{"href":"/scriptures/ot/ps/95.9"},["span.padWord","Ps."],"95:9"],"; (",["a.scriptureRef",{"href":"/scriptures/nt/heb/3.9"},"Heb. 3:9"],"; )"],["li",["i","prove"],"me … if I will not open you the windows of heaven:",["a.scriptureRef",{"href":"/scriptures/ot/mal/3.10"},"Mal. 3:10"],"; (",["a.scriptureRef",{"href":"/scriptures/bofm/3-ne/24.10"},"3 Ne. 24:10"],"; )"]]],["div",{"id":"f3","uri":"/scriptures/tg/prove.f3","order":".000.8281"},["ul.index",["li","this he said to",["i","prove"],"him:",["a.scriptureRef",{"href":"/scriptures/nt/john/6.6"},"John 6:6"],";"],["li",["i","proving"],"that this is very Christ:",["a.scriptureRef",{"href":"/scriptures/nt/acts/9.22"},"Acts 9:22"],";"],["li","Neither can they",["i","prove"],"the things whereof they now accuse me:",["a.scriptureRef",{"href":"/scriptures/nt/acts/24.13"},["span.padWord","Acts"],"24:13"],";"],["li","we have before",["i","proved"],"both Jews and Gentiles:",["a.scriptureRef",{"href":"/scriptures/nt/rom/3.9"},"Rom. 3:9"],";"],["li",["i","prove"],"what is that good:",["a.scriptureRef",{"href":"/scriptures/nt/rom/12.2"},["span.padWord","Rom."],"12:2"],";"],["li","I speak … to",["i","prove"],"the sincerity of your love:",["a.scriptureRef",{"href":"/scriptures/nt/2-cor/8.8"},"2 Cor. 8:8"],";"],["li","in the faith;",["i","prove"],"your own selves:",["a.scriptureRef",{"href":"/scriptures/nt/2-cor/13.5"},["span.padWord","2 Cor."],"13:5"],";"],["li","let every man",["i","prove"],"his own work:",["a.scriptureRef",{"href":"/scriptures/nt/gal/6.4"},"Gal. 6:4"],";"],["li",["i","Prove"],"all things:",["a.scriptureRef",{"href":"/scriptures/nt/1-thes/5.21"},"1 Thes. 5:21"],";"],["li","When your fathers tempted me,",["i","proved"],"me:",["a.scriptureRef",{"href":"/scriptures/nt/heb/3.9"},"Heb. 3:9"],";"]]],["div",{"id":"f4","uri":"/scriptures/tg/prove.f4","order":".000.8282"},["ul.index",["li","to",["i","prove"],"unto them that my words are true:",["a.scriptureRef",{"href":"/scriptures/bofm/2-ne/11.3"},"2 Ne. 11:3"],";"],["li","priestcraft … would",["i","prove"],"their entire destruction:",["a.scriptureRef",{"href":"/scriptures/bofm/alma/1.12"},"Alma 1:12"],"; (",["a.scriptureRef",{"href":"/scriptures/bofm/alma/3.8"},["span.padWord","Alma"],"3:8"],"; )"],["li","appealed unto Moses, to",["i","prove"],"that these things are true:",["a.scriptureRef",{"href":"/scriptures/bofm/alma/34.7"},["span.padWord","Alma"],"34:7"],";"],["li","Gadianton did",["i","prove"],"the overthrow … of the people of Nephi:",["a.scriptureRef",{"href":"/scriptures/bofm/hel/2.13"},"Hel. 2:13"],";"],["li","endeavoring to",["i","prove"],"by the scriptures that it was no more expedient to observe the law of Moses:",["a.scriptureRef",{"href":"/scriptures/bofm/3-ne/1.24"},"3 Ne. 1:24"],";"],["li","Gentiles have not charity … that thou wilt",["i","prove"],"them:",["a.scriptureRef",{"href":"/scriptures/bofm/ether/12.35"},"Ether 12:35"],";"],["li","pride … of the Nephites, hath",["i","proven"],"their destruction:",["a.scriptureRef",{"href":"/scriptures/bofm/moro/8.27"},"Moro. 8:27"],";"]]],["div",{"id":"f5","uri":"/scriptures/tg/prove.f5","order":".000.8283"},["ul.index",["li",["i","Proving"],"to the world that the holy scriptures are true:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/20.11"},"D&C 20:11"],";"],["li","call on the holy prophets to",["i","prove"],"his words:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/35.23"},["span.padWord","D&C"],"35:23"],";"],["li","shall be",["i","proved"],"by the Spirit through him:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/57.13"},["span.padWord","D&C"],"57:13"],";"],["li","claim their anointing if at any time they can",["i","prove"],"their lineage:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/68.21"},["span.padWord","D&C"],"68:21"],";"],["li","I send you out to",["i","prove"],"the world:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/84.79"},["span.padWord","D&C"],"84:79"],";"],["li","I will",["i","prove"],"you in all things:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/98.14"},["span.padWord","D&C"],"98:14"],";"],["li","that he may",["i","prove"],"them also:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/121.12"},["span.padWord","D&C"],"121:12"],";"],["li","that you may",["i","prove"],"yourselves unto me:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/124.55"},["span.padWord","D&C"],"124:55"],";"],["li","I did it, saith the Lord, to",["i","prove"],"you all, as I did Abraham:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/132.51"},["span.padWord","D&C"],"132:51"],";"]]],["div",{"id":"f6","uri":"/scriptures/tg/prove.f6","order":".000.8284"},["ul.index",["li","we will",["i","prove"],"them herewith:",["a.scriptureRef",{"href":"/scriptures/pgp/abr/3.25"},"Abr. 3:25"],";"]]]]],"refs":null}
