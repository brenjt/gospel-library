{"id":860,"parent_id":0,"book_id":7,"content_id":1,"language_id":1,"display_order":860,"title":"Draw, Drew, Drawn","subtitle":"","short_title":"Draw, Drew, Drawn","uri":"/scriptures/tg/draw","content":[["div",{"id":"t1","uri":"/scriptures/tg/draw.t1","order":".000.2636"},[".title",["b","Draw, Drew, Drawn"]],["div",{"id":"f1","uri":"/scriptures/tg/draw.f1","order":".000.2637"},["ul.index",["li","Moses … Because I",["i","drew"],"him out of the water:",["a.scriptureRef",{"href":"/scriptures/ot/ex/2.10"},"Ex. 2:10"],";"],["li",["i","Draw"],"nigh unto my soul:",["a.scriptureRef",{"href":"/scriptures/ot/ps/69.18"},"Ps. 69:18"],";"],["li","people",["i","draw"],"near me with their mouth:",["a.scriptureRef",{"href":"/scriptures/ot/isa/29.13"},"Isa. 29:13"],"; (",["a.scriptureRef",{"href":"/scriptures/nt/matt/15.8"},"Matt. 15:8"],";",["a.scriptureRef",{"href":"/scriptures/pgp/js-h/1.19"},"JS—H 1:19"],"; )"],["li","with lovingkindness have I",["i","drawn"],"thee:",["a.scriptureRef",{"href":"/scriptures/ot/jer/31.3"},"Jer. 31:3"],";"],["li",["i","drew"],"not near to her God:",["a.scriptureRef",{"href":"/scriptures/ot/zeph/3.2"},"Zeph. 3:2"],";"]]],["div",{"id":"f2","uri":"/scriptures/tg/draw.f2","order":".000.2638"},["ul.index",["li",["i","drew"],"his sword, and struck a servant:",["a.scriptureRef",{"href":"/scriptures/nt/matt/26.51"},"Matt. 26:51"],"; (",["a.scriptureRef",{"href":"/scriptures/nt/mark/14.47"},"Mark 14:47"],";",["a.scriptureRef",{"href":"/scriptures/nt/john/18.10"},"John 18:10"],"; )"],["li","nothing to",["i","draw"],"with, and the well is deep:",["a.scriptureRef",{"href":"/scriptures/nt/john/4.11"},"John 4:11"],";"],["li","except the Father which hath sent me",["i","draw"],"him:",["a.scriptureRef",{"href":"/scriptures/nt/john/6.44"},["span.padWord","John"],"6:44"],";"],["li","lifted up from the earth, will",["i","draw"],"all men:",["a.scriptureRef",{"href":"/scriptures/nt/john/12.32"},["span.padWord","John"],"12:32"],";"],["li",["i","drew"],"away much people af