d all things:",["a.scriptureRef",{"href":"/scriptures/bofm/3-ne/9.15"},"3 Ne. 9:15"],"; (",["a.scriptureRef",{"href":"/scriptures/bofm/morm/9.11,17"},"Morm. 9:11, 17"],"; )"],["li","all men were",["i","created"],"in the beginning after mine own image:",["a.scriptureRef",{"href":"/scriptures/bofm/ether/3.15"},"Ether 3:15"],";"]]],["div",{"id":"f5","uri":"/scriptures/tg/creation.f5","order":".000.2018"},["ul.index",["li","Jesus Christ … who",["i","created"],"the heavens and the earth:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/14.9"},"D&C 14:9"],";"],["li","he",["i","created"],"man, male and female, after his own image:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/20.18"},["span.padWord","D&C"],"20:18"],";"],["li","by the power of my Spirit",["i","created"],"I them:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/29.31"},["span.padWord","D&C"],"29:31"],";"],["li","by whom all things were",["i","made"],"which live, and move:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/45.1"},["span.padWord","D&C"],"45:1"],";"],["li","man, according to his",["i","creation"],"before the world:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/49.17"},["span.padWord","D&C"],"49:17"],";"],["li","by him … the worlds are and were",["i","created"],":",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/76.24"},["span.padWord","D&C"],"76:24"],";"],["li","he is in the sun … and the power thereof by which it was",["i","made"],":",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/88.7"},["span.padWord","D&C"],"88:7"],";"],["li","after it hath filled the measure of its",["i","creation"],":",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/88.19"},["span.padWord","D&C"],"88:19"],"; (",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/88.25"},["span.padWord","D&C"],"88:25"],"; )"],["li","worlds … men were",["i","made"],"by him:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/93.10"},["span.padWord","D&C"],"93:10"],";"],["li","Intelligence, or the light of truth, was not",["i","created"],":",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/93.29"},["span.padWord","D&C"],"93:29"],";"],["li","Lord of Sabaoth … the",["i","creator"],"of the first day:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/95.7"},["span.padWord","D&C"],"95:7"],";"],["li","I …",["i","built"],"the earth, my very handiwork:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/104.14"},["span.padWord","D&C"],"104:14"],";"],["li","Have I not",["i","made"],"the earth:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/117.6"},["span.padWord","D&C"],"117:6"],";"]]],["div",{"id":"f6","uri":"/scriptures/tg/creation.f6","order":".000.2019"},["ul.index",["li","worlds without number have I",["i","created"],":",["a.scriptureRef",{"href":"/scriptures/pgp/moses/1.33"},"Moses 1:33"],";"],["li","by mine Only Begotten I",["i","created"],"… heaven:",["a.scriptureRef",{"href":"/scriptures/pgp/moses/2.1"},["span.padWord","Moses"],"2:1"],";"],["li",["i","created"],"all things … spiritually, before they were naturally:",["a.scriptureRef",{"href":"/scriptures/pgp/moses/3.5"},["span.padWord","Moses"],"3:5"],";"],["li","out of the ground I …",["i","formed"],"every beast:",["a.scriptureRef",{"href":"/scriptures/pgp/moses/3.19"},["span.padWord","Moses"],"3:19"],";"],["li","God",["i","created"],"man, in the likeness of God:",["a.scriptureRef",{"href":"/scriptures/pgp/moses/6.8"},["span.padWord","Moses"],"6:8"],";"],["li","number of thy",["i","creations"],":",["a.scriptureRef",{"href":"/scriptures/pgp/moses/7.30"},["span.padWord","Moses"],"7:30"],";"],["li","the Gods,",["i","organized"],"and formed the heavens:",["a.scriptureRef",{"href":"/scriptures/pgp/abr/4.1"},"Abr. 4:1"],";"]]],[".reference","See also",["a.scriptureRef",{"href":"/scriptures/ot/gen/2.1"},"Gen. 2:1"],";",["a.scriptureRef",{"href":"/scriptures/ot/prov/8.22"},"Prov. 8:22"],";",["a.scriptureRef",{"href":"/scriptures/nt/rom/1.20"},"Rom. 1:20"],";",["a.scri