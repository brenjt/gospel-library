hese",["i","miracles"],"… except God be with him:",["a.scriptureRef",{"href":"/scriptures/nt/john/3.2"},["span.padWord","John"],"3:2"],";"],["li","second",["i","miracle"],"that Jesus did:",["a.scriptureRef",{"href":"/scriptures/nt/john/4.54"},["span.padWord","John"],"4:54"],";"],["li","followed him, because they saw his",["i","miracles"],":",["a.scriptureRef",{"href":"/scriptures/nt/john/6.2"},["span.padWord","John"],"6:2"],";"],["li","when they had seen the",["i","miracle"],"that Jesus did:",["a.scriptureRef",{"href":"/scriptures/nt/john/6.14"},["span.padWord","John"],"6:14"],";"],["li","Ye seek me, not because ye saw the",["i","miracles"],":",["a.scriptureRef",{"href":"/scriptures/nt/john/6.26"},["span.padWord","John"],"6:26"],";"],["li","When Christ cometh, will he do more",["i","miracles"],":",["a.scriptureRef",{"href":"/scriptures/nt/john/7.31"},["span.padWord","John"],"7:31"],";"],["li","How can a man that is a sinner do such",["i","miracles"],":",["a.scriptureRef",{"href":"/scriptures/nt/john/9.16"},["span.padWord","John"],"9:16"],";"],["li",["i","works"],"that I do in my Father’s name:",["a.scriptureRef",{"href":"/scriptures/nt/john/10.25"},["span.padWord","John"],"10:25"],";"],["li","John did no",["i","miracle"],":",["a.scriptureRef",{"href":"/scriptures/nt/john/10.41"},["span.padWord","John"],"10:41"],";"],["li","this man doeth many",["i","miracles"],":",["a.scriptureRef",{"href":"/scriptures/nt/john/11.47"},["span.padWord","John"],"11:47"],";"],["li","he had done so many",["i","miracles"],"before them:",["a.scriptureRef",{"href":"/scriptures/nt/john/12.37"},["span.padWord","John"],"12:37"],";"],["li","man approved of God among you by",["i","miracles"],":",["a.scriptureRef",{"href":"/scriptures/nt/acts/2.22"},"Acts 2:22"],";"],["li","notable",["i","miracle"],"hath been done:",["a.scriptureRef",{"href":"/scriptures/nt/acts/4.16"},["span.padWord","Acts"],"4:16"],";"],["li","Stephen … did great wonders and",["i","miracles"],":",["a.scriptureRef",{"href":"/scr