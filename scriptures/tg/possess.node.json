{"id":2333,"parent_id":0,"book_id":7,"content_id":1,"language_id":1,"display_order":2333,"title":"Possess","subtitle":"","short_title":"Possess","uri":"/scriptures/tg/possess","content":[["div",{"id":"t1","uri":"/scriptures/tg/possess.t1","order":".000.7870"},[".title",["b","Possess"]],[".reference","See also",["a.scriptureRef",{"href":"/scriptures/tg/inherit"},"Inherit"],";",["a.scriptureRef",{"href":"/scriptures/tg/possession"},"Possession"],";",["a.scriptureRef",{"href":"/scriptures/tg/possessor"},"Possessor"]],["div",{"id":"f2","uri":"/scriptures/tg/possess.f2","order":".000.7871"},["ul.index",["li","thy seed shall",["i","possess"],"the gate of his enemies:",["a.scriptureRef",{"href":"/scriptures/ot/gen/22.17"},"Gen. 22:17"],";"],["li","Let us go up … and",["i","possess"],"it:",["a.scriptureRef",{"href":"/scriptures/ot/num/13.30"},"Num. 13:30"],";"],["li","Caleb … and his seed shall",["i","possess"],"it:",["a.scriptureRef",{"href":"/scriptures/ot/num/14.24"},["span.padWord","Num."],"14:24"],";"],["li","go in and",["i","possess"],"the land:",["a.scriptureRef",{"href":"/scriptures/ot/deut/1.8"},"Deut. 1:8"],"; (",["a.scriptureRef",{"href":"/scriptures/ot/deut/4.1"},["span.padWord","Deut."],"4:1"],";",["a.scriptureRef",{"href":"/scriptures/ot/deut/8.1"},["span.padWord","Deut."],"8:1"],";",["a.scriptureRef",{"href":"/scriptures/ot/deut/11.8"},["span.padWord","Deut."],"11:8"],";",["a.scriptureRef",{"href":"/scriptures/ot/neh/9.15"},"Neh. 9:15"],"; )"],["li","go in to",["i","possess"],"nations greater and mightier:",["a.scriptureRef",{"href":"/scriptures/ot/deut/9.1"},["span.padWord","Deut."],"9:1"],"; (",["a.scriptureRef",{"href":"/scriptures/ot/deut/11.23"},["span.padWord","Deut."],"11:23"],"; )"],["li","God giveth you to",["i","possess"],"it:",["a.scriptureRef",{"href":"/scriptures/ot/josh/1.11"},"Josh. 1:11"],";"],["li","thou hast",["i","possessed"],"my reins:",["a.scriptureRef",{"href":"/scriptures/ot/ps/139.13"},"Ps. 139:13"],";"],["li","they shall",["i","posses