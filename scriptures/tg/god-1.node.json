{"id":1255,"parent_id":0,"book_id":7,"content_id":1,"language_id":1,"display_order":1255,"title":"god","subtitle":"","short_title":"god","uri":"/scriptures/tg/god-1","content":[["div",{"id":"t1","uri":"/scriptures/tg/god-1.t1","order":".000.3986"},[".title",["b","god"]],[".reference","See also",["a.scriptureRef",{"href":"/scriptures/tg/god-2"},"God"],";",["a.scriptureRef",{"href":"/scriptures/tg/idolatry"},"Idolatry"],";",["a.scriptureRef",{"href":"/scriptures/tg/image"},"Image"]],["div",{"id":"f2","uri":"/scriptures/tg/god-1.f2","order":".000.3987"},["ul.index",["li","Put away the strange",["i","gods"],":",["a.scriptureRef",{"href":"/scriptures/ot/gen/35.2"},"Gen. 35:2"],"; (",["a.scriptureRef",{"href":"/scriptures/ot/1-sam/7.3"},"1 Sam. 7:3"],"; )"],["li","against all the",["i","gods"],"of Egypt I will execute judgment:",["a.scriptureRef",{"href":"/scriptures/ot/ex/12.12"},"Ex. 12:12"],";"],["li","Lord is greater than all",["i","gods"],":",["a.scriptureRef",{"href":"/scriptures/ot/ex/18.11"},["span.padWord","Ex."],"18:11"],";"],["li","shalt have no other",["i","gods"],"before me:",["a.scriptureRef",{"href":"/scriptures/ot/ex/20.3"},["span.padWord","Ex."],"20:3"],"; (",["a.scriptureRef",{"href":"/scriptures/ot/ex/34.14"},["span.padWord","Ex."],"34:14"],";",["a.scriptureRef",{"href":"/scriptures/ot/deut/5.7"},"Deut. 5:7"],"; )"],["li","no mention of the name of other",["i","gods"],":",["a.scriptureRef",{"href":"/scriptures/ot/ex/23.13"},["span.padWord","Ex."],"23:13"],";"],["li","make us",["i","gods"],", which shall go before:",["a.scriptureRef",{"href":"/scriptures/ot/ex/32.1"},["span.padWord","Ex."],"32:1"],"; (",["a.scriptureRef",{"href":"/scriptures/nt/acts/7.40"},"Acts 7:40"],"; )"],["li","shalt worship no other",["i","god"],":",["a.scriptureRef",{"href":"/scriptures/ot/ex/34.14"},["span.padWord","Ex."],"34:14"],";"],["li","shall serve",["i","gods"],", the work of men’s hands:",["a.scriptureRef",{"href":"/scriptures/ot/deut/4.28"},"Deut. 4:28"],";"],["li","to go after other",["i","gods"],"to serve them:",["a.scriptureRef",{"href":"/scriptures/ot/deut/28.14"},["span.padWord","Deut."],"28:14"],";"],["li","sacrificed … to",["i","gods"],"whom they knew not:",["a.scriptureRef",{"href":"/scriptures/ot/deut/32.17"},["span.padWord","Deut."],"32:17"],";"],["li","Elijah said … call on the name of your",["i","gods"],":",["a.scriptureRef",{"href":"/scriptures/ot/1-kgs/18.25"},"1 Kgs. 18:25"],";"],["li","Cry aloud: for he is a",["i","god"],":",["a.scriptureRef",{"href":"/scriptures/ot/1-kgs/18.27"},["span.padWord","1 Kgs."],"18:27"],";"],["li","he judgeth among the",["i","gods"],":",["a.scriptureRef",{"href":"/scriptures/ot/ps/82.1"},"Ps. 82:1"],";"],["li","I have said, Ye are",["i","gods"],":",["a.scriptureRef",{"href":"/scriptures/ot/ps/82.6"},["span.padWord","Ps."],"82:6"],"; (",["a.scriptureRef",{"href":"/scriptures/nt/john/10.34"},"John 10:34"],";",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/76.58"},"D&C 76:58"],"; )"],["li","Among the",["i","gods"],"there is none like unto thee:",["a.scriptureRef",{"href":"/scriptures/ot/ps/86.8"},["span.padWord","Ps."],"86:8"],";"],["li","give thanks unto the God of",["i","gods"],":",["a.scriptureRef",{"href":"/scriptures/ot/ps/136.2"},["span.padWord","Ps."],"136:2"],";"],["li","cast their",["i","gods"],"into the fire: for they were no",["i","gods"],":",["a.scriptureRef",{"href":"/scriptures/ot/isa/37.19"},"Isa. 37:19"],";"],["li","changed their",["i","gods"],", which are yet no",["i","gods"],":",["a.scriptureRef",{"href":"/scriptures/ot/jer/2.11"},"Jer. 2:11"],";"],["li","I am a",["i","God"],", I sit in the seat of God:",["a.scriptureRef",{"href":"/scriptures/ot/ezek/28.2"},"Ezek. 28:2"],";"],["li","thou hast praised the",["i","gods"],"of silver:",["a.scriptureRef",{"href":"/scriptures/ot/dan/5.23"},"Dan. 5:23"],";"]]],["div",{"id":"f3","uri":"/scriptures/tg/god-1.f3","order":".000.3988"},["ul.index",["li",["i","gods"],"are come down to us in the likeness of men:",["a.scriptureRef",{"href":"/scriptures/nt/acts/14.11"},"Acts 14:11"],";"],["li","seemeth to be a setter forth of strange",["i","gods"],":",["a.scriptureRef",{"href":"/scriptures/nt/acts/17.18"},["span.padWord","Acts"],"17:18"],";"],["li","changed their minds, and said that he was a",["i","god"],":",["a.scriptureRef",{"href":"/scriptures/nt/acts/28.6"},["span.padWord","Acts"],"28:6"],";"],["li","though there be that are called",["i","gods"],":",["a.scriptureRef",{"href":"/scriptures/nt/1-cor/8.5"},"1 Cor. 8:5"],";"],["li",["i","god"],"of this world hath blinded the minds:",["a.scriptureRef",{"href":"/scriptures/nt/2-cor/4.4"},"2 Cor. 4:4"],";"],["li","service unto them which by nature are no",["i","gods"],":",["a.scriptureRef",{"href":"/scriptures/nt/gal/4.8"},"Gal. 4:8"],";"]]],["div",{"id":"f4","uri":"/scriptures/tg/god-1.f4","order":".000.3989"},["ul.index",["li","sacrifices unto their idol",["i","gods"],":",["a.scriptureRef",{"href":"/scriptures/bofm/morm/4.14"},"Morm. 4:14"],";"]]],["div",{"id":"f5","uri":"/scriptures/tg/god-1.f5","order":".000.3990"},["ul.index",["li","Then shall they be",["i","gods"],", because they have no end:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/132.20"},"D&C 132:20"],";"]]],["div",{"id":"f6","uri":"/scriptures/tg/god-1.f6","order":".000.3991"},["ul.index",["li","worshiping of the",["i","gods"],"of the heathen:",["a.scriptureRef",{"href":"/scriptures/pgp/abr/1.5"},"Abr. 1:5"],"; (",["a.scriptureRef",{"href":"/scriptures/pgp/abr/1.8"},["span.padWord","Abr."],"1:8"],"; )"],["li","they would not … worship",["i","gods"],"of wood:",["a.scriptureRef",{"href":"/scriptures/pgp/abr/1.11"},["span.padWord","Abr."],"1:11"],";"]]]]],"refs":null}
