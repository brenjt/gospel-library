li","ye cannot behold design of God with",["i","natural"],"eyes:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/58.3"},["span.padWord","D&C"],"58:3"],";"],["li","man cannot see God with carnal or",["i","natural"],"mind:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/67.10"},["span.padWord","D&C"],"67:10"],";"],["li",["i","natural"],"man cannot abide presence of God:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/67.12"},["span.padWord","D&C"],"67:12"],";"],["li","celestial spirit shall receive same body which was",["i","natural"],"body:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/88.28"},["span.padWord","D&C"],"88:28"],";"],["li",["i","nature"],"and disposition of almost all men to exercise unrighteous dominion:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/121.39"},["span.padWord","D&C"],"121:39"],";"]]],["div",{"id":"f4","uri":"/scriptures/triple-index/nature.f4","order":".000.4542"},["ul.index",["li","Moses could not behold God with",["i","natural"],"eyes:",["a.scriptureRef",{"href":"/scriptures/pgp/moses/1.11"},"Moses 1:11"],";"],["li","Moses beholds things not visible to",["i","natural"],"eye:",["a.scriptureRef",{"href":"/scriptures/pgp/moses/6.36"},["span.padWord","Moses"],"6:36"],";"]]]]],"refs":null}
