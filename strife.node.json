],"80:6"],";"],["li","angered him also at the waters of",["i","strife"],":",["a.scriptureRef",{"href":"/scriptures/ot/ps/106.32"},["span.padWord","Ps."],"106:32"],";"],["li","Hatred stirreth up",["i","strifes"],":",["a.scriptureRef",{"href":"/scriptures/ot/prov/10.12"},"Prov. 10:12"],";"],["li","wrathful man stirreth up",["i","strife"],":",["a.scriptureRef",{"href":"/scriptures/ot/prov/15.18"},["span.padWord","Prov."],"15:18"],";"],["li","froward man soweth",["i","strife"],":",["a.scriptureRef",{"href":"/scriptures/ot/prov/16.28"},["span.padWord","Prov."],"16:28"],";"],["li","house full of sacrifices with",["i","strife"],":",["a.scriptureRef",{"href":"/scriptures/ot/prov/17.1"},["span.padWord","Prov."],"17:1"],";"],["li","He loveth transgression that loveth",["i","strife"],":",["a.scriptureRef",{"href":"/scriptures/ot/prov/17.19"},["span.padWord","Prov."],"17:19"],";"],["li","honour for a man to cease from",["i","strife"],":",["a.scriptureRef",{"href":"/scriptures/ot/prov/20.3"},["span.padWord","Prov."],"20:3"],";"],["li",["i","strife"],"and reproach shall cease:",["a.scriptureRef",{"href":"/scriptures/ot/prov/22.10"},["span.padWord","Prov."],"22:10"],";"],["li","where there is no talebearer, the",["i","strife"],"ceaseth:",["a.scriptureRef",{"href":"/scriptures/ot/prov/26.20"},["span.padWord","Prov."],"26:20"],";"],["li","proud heart stirreth up",["i","strife"],":",["a.scriptureRef",{"href":"/scriptures/ot/prov/28.25"},["span.padWord","Prov."],"28:25"],";"],["li","angry man stirreth up",["i","strife"],":",["a.scriptureRef",{"href":"/scriptures/ot/prov/29.22"},["span.padWord","Prov."],"29:22"],";"],["li","forcing of wrath bringeth forth",["i","strife"],":",["a.scriptureRef",{"href":"/scriptures/ot/prov/30.33"},["span.padWord","Prov."],"30:33"],";"],["li","ye fast for",["i","strife"],"and debate:",["a.scriptureRef",{"href":"/scriptures/ot/isa/58.4"},"Isa. 58:4"],";"],["li","hast borne me a man of",["i","strife"],":",["a.scriptureRef",{"href":"/scriptures/ot/jer/15.10"