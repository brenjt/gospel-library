{"id":728,"parent_id":0,"book_id":7,"content_id":1,"language_id":1,"display_order":728,"title":"Death","subtitle":"","short_title":"Death","uri":"/scriptures/tg/death","content":[["div",{"id":"t1","uri":"/scriptures/tg/death.t1","order":".000.2202"},[".title",["b","Death"]],[".reference","See also",["a.scriptureRef",{"href":"/scriptures/tg/dead"},"Dead"],";",["a.scriptureRef",{"href":"/scriptures/tg/death-power-over"},"Death, Power over"],";",["a.scriptureRef",{"href":"/scriptures/tg/death-spiritual-first"},"Death, Spiritual, First"],";",["a.scriptureRef",{"href":"/scriptures/tg/death-spiritual-second"},"Death, Spiritual, Second"],";",["a.scriptureRef",{"href":"/scriptures/tg/die"},"Die"],";",["a.scriptureRef",{"href":"/scriptures/tg/fall-of-man"},"Fall of Man"],";",["a.scriptureRef",{"href":"/scriptures/tg/grave-noun"},"Grave [",["i","noun"],"]"],";",["a.scriptureRef",{"href":"/scriptures/tg/immortality"},"Immortality"],";",["a.scriptureRef",{"href":"/scriptures/tg/mortality"},"Mortality"]],["div",{"id":"f2","uri":"/scriptures/tg/death.f2","order":".000.2203"},["ul.index",["li","giveth any of his seed unto Molech; he shall surely be put to",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/ot/lev/20.2"},"Lev. 20:2"],";"],["li","fathers shall not be put to",["i","death"],"for the children:",["a.scriptureRef",{"href":"/scriptures/ot/deut/24.16"},"Deut. 24:16"],"; (",["a.scriptureRef",{"href":"/scriptures/ot/2-kgs/14.6"},"2 Kgs. 14:6"],"; )"],["li","set before you life and",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/ot/deut/30.19"},["span.padWord","Deut."],"30:19"],";"],["li","rebel … shall be put to",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/ot/josh/1.18"},"Josh. 1:18"],";"],["li","this day I am going the way of all the",["i","earth"],":",["a.scriptureRef",{"href":"/scriptures/ot/josh/23.14"},["span.padWord","Josh."],"23:14"],";"],["li","in their",["i","death"],"they were not divided:",["a.scriptureRef",{"href":"/scriptures/ot/2-sam/1.23"},"2 Sam. 1:23"],";"],["li","there is",["i","death"],"in the pot:",["a.scriptureRef",{"href":"/scriptures/ot/2-kgs/4.40"},"2 Kgs. 4:40"],";"],["li","every man shall be put to",["i","death"],"for his own sin:",["a.scriptureRef",{"href":"/scriptures/ot/2-kgs/14.6"},["span.padWord","2 Kgs."],"14:6"],";"],["li","not seek the Lord … be put to",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/ot/2-chr/15.13"},"2 Chr. 15:13"],";"],["li","land of darkness and the shadow of",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/ot/job/10.21"},"Job 10:21"],";"],["li","go the",["i","way"],"whence I shall not return:",["a.scriptureRef",{"href":"/scriptures/ot/job/16.22"},["span.padWord","Job"],"16:22"],";"],["li","flesh shall",["i","perish"],"… and man shall turn again unto dust:",["a.scriptureRef",{"href":"/scriptures/ot/job/34.15"},["span.padWord","Job"],"34:15"],";"],["li","valley of the shadow of",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/ot/ps/23.4"},"Ps. 23:4"],";"],["li","deliver their soul from",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/ot/ps/33.19"},["span.padWord","Ps."],"33:19"],";"],["li","loose those that are appointed to",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/ot/ps/102.20"},["span.padWord","Ps."],"102:20"],";"],["li","Precious in the sight of the Lord is the",["i","death"],"of his saints:",["a.scriptureRef",{"href":"/scriptures/ot/ps/116.15"},["span.padWord","Ps."],"116:15"],";"],["li","Her feet go down to",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/ot/prov/5.5"},"Prov. 5:5"],";"],["li","they that hate me love",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/ot/prov/8.36"},["span.padWord","Prov."],"8:36"],";"],["li","righteousness delivereth from",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/ot/prov/10.2"},["span.padWord","Prov."],"10:2"],";"],["li","end thereof are the ways of",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/ot/prov/14.12"},["span.padWord","Prov."],"14:12"],";"],["li","the righteous hath hope in his",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/ot/prov/14.32"},["span.padWord","Prov."],"14:32"],";"],["li",["i","Death"],"and life are in the power of the tongue:",["a.scriptureRef",{"href":"/scriptures/ot/prov/18.21"},["span.padWord","Prov."],"18:21"],";"],["li","to deliver them that are drawn unto",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/ot/prov/24.11"},["span.padWord","Prov."],"24:11"],";"],["li","better … the day of",["i","death"],"than the day of one’s birth:",["a.scriptureRef",{"href":"/scriptures/ot/eccl/7.1"},"Eccl. 7:1"],";"],["li","dust return to the earth …",["i","spirit"],"shall return unto God:",["a.scriptureRef",{"href":"/scriptures/ot/eccl/12.7"},["span.padWord","Eccl."],"12:7"],";"],["li","land of the shadow of",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/ot/isa/9.2"},"Isa. 9:2"],"; (",["a.scriptureRef",{"href":"/scriptures/nt/matt/4.16"},"Matt. 4:16"],"; )"],["li","He will swallow up",["i","death"],"in victory:",["a.scriptureRef",{"href":"/scriptures/ot/isa/25.8"},["span.padWord","Isa."],"25:8"],"; (",["a.scriptureRef",{"href":"/scriptures/nt/1-cor/15.54"},"1 Cor. 15:54"],"; )"],["li","made his grave with the wicked, and with the rich in his",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/ot/isa/53.9"},["span.padWord","Isa."],"53:9"],"; (",["a.scriptureRef",{"href":"/scriptures/bofm/mosiah/14.9"},"Mosiah 14:9"],"; )"],["li","he hath poured out his soul unto",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/ot/isa/53.12"},["span.padWord","Isa."],"53:12"],"; (",["a.scriptureRef",{"href":"/scriptures/bofm/mosiah/14.12"},"Mosiah 14:12"],"; )"],["li",["i","death"],"shall be chosen rather than life:",["a.scriptureRef",{"href":"/scriptures/ot/jer/8.3"},"Jer. 8:3"],";"],["li","way of life, and the way of",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/ot/jer/21.8"},["span.padWord","Jer."],"21:8"],";"],["li","if ye put me to",["i","death"],"… bring innocent blood upon yourselves:",["a.scriptureRef",{"href":"/scriptures/ot/jer/26.15"},["span.padWord","Jer."],"26:15"],";"],["li","I have no pleasure in the",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/ot/ezek/18.32"},"Ezek. 18:32"],"; (",["a.scriptureRef",{"href":"/scriptures/ot/ezek/33.11"},["span.padWord","Ezek."],"33:11"],"; )"],["li","I will redeem them from",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/ot/hosea/13.14"},"Hosea 13:14"],";"]]],["div",{"id":"f3","uri":"/scriptures/tg/death.f3","order":".000.2204"},["ul.index",["li","brother shall deliver up the brother to",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/matt/10.21"},"Matt. 10:21"],"; (",["a.scriptureRef",{"href":"/scriptures/nt/mark/13.12"},"Mark 13:12"],"; )"],["li","children shall rise up against their parents, and cause them to be put to",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/matt/10.21"},["span.padWord","Matt."],"10:21"],"; (",["a.scriptureRef",{"href":"/scriptures/nt/mark/13.12"},"Mark 13:12"],";",["a.scriptureRef",{"href":"/scriptures/nt/luke/21.16"},"Luke 21:16"],"; )"],["li","shall not taste of",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/matt/16.28"},["span.padWord","Matt."],"16:28"],"; (",["a.scriptureRef",{"href":"/scriptures/nt/mark/9.1"},"Mark 9:1"],";",["a.scriptureRef",{"href":"/scriptures/nt/luke/9.27"},"Luke 9:27"],"; )"],["li","condemn him to",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/matt/20.18"},["span.padWord","Matt."],"20:18"],"; (",["a.scriptureRef",{"href":"/scriptures/nt/mark/10.33"},"Mark 10:33"],";",["a.scriptureRef",{"href":"/scriptures/nt/luke/18.33"},"Luke 18:33"],"; )"],["li","exceeding sorrowful, even unto",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/matt/26.38"},["span.padWord","Matt."],"26:38"],"; (",["a.scriptureRef",{"href":"/scriptures/nt/mark/14.34"},"Mark 14:34"],"; )"],["li","little daughter lieth at the point of",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/mark/5.23"},"Mark 5:23"],";"],["li","take him by craft, and put him to",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/mark/14.1"},["span.padWord","Mark"],"14:1"],";"],["li","sit in darkness and in the shadow of",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/luke/1.79"},"Luke 1:79"],";"],["li","not see",["i","death"],", before he had seen the Lord’s Christ:",["a.scriptureRef",{"href":"/scriptures/nt/luke/2.26"},["span.padWord","Luke"],"2:26"],";"],["li","not taste of",["i","death"],", till they see the kingdom:",["a.scriptureRef",{"href":"/scriptures/nt/luke/9.27"},["span.padWord","Luke"],"9:27"],";"],["li","he gave up the",["i","ghost"],":",["a.scriptureRef",{"href":"/scriptures/nt/luke/23.46"},["span.padWord","Luke"],"23:46"],";"],["li","passed from",["i","death"],"unto life:",["a.scriptureRef",{"href":"/scriptures/nt/john/5.24"},"John 5:24"],";"],["li","he shall never see",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/john/8.51"},["span.padWord","John"],"8:51"],";"],["li","counsel together for to put him to",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/john/11.53"},["span.padWord","John"],"11:53"],";"],["li","signifying what",["i","death"],"he should die:",["a.scriptureRef",{"href":"/scriptures/nt/john/12.33"},["span.padWord","John"],"12:33"],";"],["li","not lawful for us to put any man to",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/john/18.31"},["span.padWord","John"],"18:31"],";"],["li","signifying by what",["i","death"],"he should glorify God:",["a.scriptureRef",{"href":"/scriptures/nt/john/21.19"},["span.padWord","John"],"21:19"],";"],["li","Saul was consenting unto his",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/acts/8.1"},"Acts 8:1"],"; (",["a.scriptureRef",{"href":"/scriptures/nt/acts/22.20"},["span.padWord","Acts"],"22:20"],"; )"],["li","commanded that they should be put to",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/acts/12.19"},["span.padWord","Acts"],"12:19"],";"],["li","nothing laid to his charge worthy of",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/acts/23.29"},["span.padWord","Acts"],"23:29"],";"],["li","he had committed nothing worthy of",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/acts/25.25"},["span.padWord","Acts"],"25:25"],";"],["li","which commit such things are worthy of",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/rom/1.32"},"Rom. 1:32"],";"],["li","reconciled to God by the",["i","death"],"of his Son:",["a.scriptureRef",{"href":"/scriptures/nt/rom/5.10"},["span.padWord","Rom."],"5:10"],";"],["li",["i","death"],"passed upon all men:",["a.scriptureRef",{"href":"/scriptures/nt/rom/5.12"},["span.padWord","Rom."],"5:12"],";"],["li",["i","death"],"reigned from Adam to Moses:",["a.scriptureRef",{"href":"/scriptures/nt/rom/5.14"},["span.padWord","Rom."],"5:14"],";"],["li","planted together in the likeness of his",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/rom/6.5"},["span.padWord","Rom."],"6:5"],";"],["li","wages of sin is",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/rom/6.23"},["span.padWord","Rom."],"6:23"],";"],["li","carnally minded is",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/rom/8.6"},["span.padWord","Rom."],"8:6"],"; (",["a.scriptureRef",{"href":"/scriptures/bofm/2-ne/9.39"},"2 Ne. 9:39"],"; )"],["li","I am persuaded, that neither",["i","death"],", nor life:",["a.scriptureRef",{"href":"/scriptures/nt/rom/8.38"},["span.padWord","Rom."],"8:38"],";"],["li","ye do shew the Lord’s",["i","death"],"till he come:",["a.scriptureRef",{"href":"/scriptures/nt/1-cor/11.26"},"1 Cor. 11:26"],";"],["li","by man came",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/1-cor/15.21"},["span.padWord","1 Cor."],"15:21"],";"],["li","last enemy that shall be destroyed is",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/1-cor/15.26"},["span.padWord","1 Cor."],"15:26"],";"],["li","we had the sentence of",["i","death"],"in ourselves:",["a.scriptureRef",{"href":"/scriptures/nt/2-cor/1.9"},"2 Cor. 1:9"],";"],["li","To the one we are the savour of",["i","death"],"unto",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/2-cor/2.16"},["span.padWord","2 Cor."],"2:16"],";"],["li","sorrow of the world worketh",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/2-cor/7.10"},["span.padWord","2 Cor."],"7:10"],";"],["li","Christ shall be magnified in my body, whether it be by life, or by",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/philip/1.20"},"Philip. 1:20"],";"],["li","obedient unto",["i","death"],", even the",["i","death"],"of the cross:",["a.scriptureRef",{"href":"/scriptures/nt/philip/2.8"},["span.padWord","Philip."],"2:8"],";"],["li","know him … made conformable unto his",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/philip/3.10"},["span.padWord","Philip."],"3:10"],";"],["li","Christ, who hath abolished",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/2-tim/1.10"},"2 Tim. 1:10"],";"],["li","suffering of",["i","death"],"… taste",["i","death"],"for every man:",["a.scriptureRef",{"href":"/scriptures/nt/heb/2.9"},"Heb. 2:9"],";"],["li","through",["i","death"],"he might destroy him that had the power of",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/heb/2.14"},["span.padWord","Heb."],"2:14"],";"],["li","by means of",["i","death"],", for the redemption:",["a.scriptureRef",{"href":"/scriptures/nt/heb/9.15"},["span.padWord","Heb."],"9:15"],";"],["li","where a testament is, there must … be the",["i","death"],"of the testator:",["a.scriptureRef",{"href":"/scriptures/nt/heb/9.16"},["span.padWord","Heb."],"9:16"],";"],["li","Enoch was translated that he should not see",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/heb/11.5"},["span.padWord","Heb."],"11:5"],";"],["li","sin … bringeth forth",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/james/1.15"},"James 1:15"],";"],["li","converteth the sinner … save a soul from",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/james/5.20"},["span.padWord","James"],"5:20"],";"],["li","put to",["i","death"],"in the flesh, but quickened by the Spirit:",["a.scriptureRef",{"href":"/scriptures/nt/1-pet/3.18"},"1 Pet. 3:18"],";"],["li","passed from",["i","death"],"unto life:",["a.scriptureRef",{"href":"/scriptures/nt/1-jn/3.14"},"1 Jn. 3:14"],";"],["li","a sin which is not unto",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/1-jn/5.16"},["span.padWord","1 Jn."],"5:16"],";"],["li","keys of hell and of",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/rev/1.18"},"Rev. 1:18"],";"],["li","be thou faithful unto",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/rev/2.10"},["span.padWord","Rev."],"2:10"],";"],["li","his name that sat on him was",["i","Death"],":",["a.scriptureRef",{"href":"/scriptures/nt/rev/6.8"},["span.padWord","Rev."],"6:8"],";"],["li","men seek",["i","death"],", and shall not find it:",["a.scriptureRef",{"href":"/scriptures/nt/rev/9.6"},["span.padWord","Rev."],"9:6"],";"],["li","they loved not their lives unto the",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/rev/12.11"},["span.padWord","Rev."],"12:11"],";"],["li","on such the second",["i","death"],"hath no power:",["a.scriptureRef",{"href":"/scriptures/nt/rev/20.6"},["span.padWord","Rev."],"20:6"],";"],["li",["i","death"],"and hell delivered up the dead:",["a.scriptureRef",{"href":"/scriptures/nt/rev/20.13"},["span.padWord","Rev."],"20:13"],";"],["li","shall be no more",["i","death"],", neither sorrow:",["a.scriptureRef",{"href":"/scriptures/nt/rev/21.4"},["span.padWord","Rev."],"21:4"],";"],["li","which is the second",["i","death"],":",["a.scriptureRef",{"href":"/scriptures/nt/rev/21.8"},["span.padWord","Rev."],"21:8"],";"]]],["div",{"id":"f4","uri":"/scriptures/tg/death.f4","order":".000.2205"},["ul.index",["li","cold and silent",["i","grav