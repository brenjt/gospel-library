},["span.padWord","1 Cor."],"14:34"],";"],["li","his Son, made of a",["i","woman"],":",["a.scriptureRef",{"href":"/scriptures/nt/gal/4.4"},"Gal. 4:4"],";"],["li",["i","women"],"adorn themselves in modest apparel:",["a.scriptureRef",{"href":"/scriptures/nt/1-tim/2.9"},"1 Tim. 2:9"],";"],["li","Let the",["i","woman"],"learn in silence:",["a.scriptureRef",{"href":"/scriptures/nt/1-tim/2.11"},["span.padWord","1 Tim."],"2:11"],";"],["li","younger",["i","women"],"marry, bear children:",["a.scriptureRef",{"href":"/scriptures/nt/1-tim/5.14"},["span.padWord","1 Tim."],"5:14"],";"],["li","lead captive silly",["i","women"],"laden with sins:",["a.scriptureRef",{"href":"/scriptures/nt/2-tim/3.6"},"2 Tim. 3:6"],";"],["li","aged",["i","women"],"likewise, that they be in behaviour:",["a.scriptureRef",{"href":"/scriptures/nt/titus/2.3"},"Titus 2:3"],";"],["li","teach the young",["i","women"],"to be sober:",["a.scriptureRef",{"href":"/scriptures/nt/titus/2.4"},["span.padWord","Titus"],"2:4"],";"],["li",["i","Women"],"received their dead raised to life:",["a.scriptureRef",{"href":"/scriptures/nt/heb/11.35"},"Heb. 11:35"],";"],["li","holy",["i","women"],"also, who trusted in God:",["a.scriptureRef",{"href":"/scriptures/nt/1-pet/3.5"},"1 Pet. 3:5"],";"],["li",["i","woman"],"clothed with the sun:",["a.scriptureRef",{"href":"/scriptures/nt/rev/12.1"},"Rev. 12:1"],";"],["li","not defiled with",["i","women"],"; for they are virgins:",["a.scriptureRef",{"href":"/scriptures/nt/rev/14.4"},["span.padWord","Rev."],"14:4"],";"],["li","saw a",["i","woman"],"sit upon a scarlet coloured beast:",["a.scriptureRef",{"href":"/scriptures/nt/rev/17.3"},["span.padWord","Rev."],"17:3"],";"]]],["div",{"id":"f4","uri":"/scriptures/tg/woman.f4","order":".000.12128"},["ul.index",["li",["i","women"],"did bear children in the wilderness:",["a.scriptureRef",{"href":"/scriptures/bofm/1-ne/17.1"},"1 Ne. 17:1"],";"],["li","our",["i","women"],"have toiled, being big with child:",["a.scriptureRef",{"href":"/scriptures/bofm/1-ne/17.20"},["span.padWord","1 Ne."],"17:20"],";"],["li","he suffereth the pains of all … men,",["i","women"],", and children:",["a.scriptureRef",{"href":"/scriptures/bofm/2-ne/9.21"},"2 Ne. 9:21"],";"],["li","Lord God, delight in the chastity of",["i","women"],":",["a.scriptureRef",{"href":"/scriptures/bofm/jacob/2.28"},"Jacob 2:28"],";"],["li",["i","women"],"should spin, and toil, and work:",["a.scriptureRef",{"href":"/scriptures/bofm/mosiah/10.5"},"Mosiah 10:5"],";"],["li",["i","woman"],", there has not been such great faith among all:",["a.scriptureRef",{"href":"/scriptures/bofm/alma/19.10"},"Alma 19:10"],";"],["li",["i","woman"],"servant who had caused the multitude to be gathered:",["a.scriptureRef",{"href":"/scriptures/bofm/alma/19.28"},["span.padWord","Alma"],"19:28"],";"],["li","imparteth his word by angels unto …",["i","women"],"also:",["a.scriptureRef",{"href":"/scriptures/bofm/alma/32.23"},["span.padWord","Alma"],"32:23"],";"],["li","there was not a",["i","woman"],"nor a child among all:",["a.scriptureRef",{"href":"/scriptures/bofm/alma/54.3"},["span.padWord","Alma"],"54:3"],";"],["li","Lord hath called thee as a",["i","woman"],"forsaken:",["a.scriptureRef",{"href":"/scriptures/bofm/3-ne/22.6"},"3 Ne. 22:6"],";"]]],["div",{"id":"f5","uri":"/scriptures/tg/woman.f5","order":".000.12129"},["ul.index",["li","all … repent and be baptized, and not only men, but",["i","women"],":",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/18.42"},"D&C 18:42"],";"],["li","elect",["i","lady"],", whom I have called:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/25.3"},["span.padWord","D&C"],"25:3"],";"],["li","if a man or",["i","woman"],"shall rob … delivered up unto the law of the land:",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/42.84"},["span.padWord","D&C"],"42:84"],";"],["li","Son of Man cometh not in the form of a",["i","woman"],":",["a.scriptureRef",{"href":"/scriptures/dc-testament/dc/49.22"},["span.padWord","D&C"],"49:22"],";"],["l